{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "python scripts/strategy_precalculator.py --continuous",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  },
  "envVars": {
    "SERVICE_TYPE": {
      "description": "Identifies this as the pre-calculator service",
      "default": "precalculator"
    }
  }
}
